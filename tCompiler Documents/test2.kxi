    //do
    //construct itself
    symboltable(){
        count = 1;
    }
    //generate symid
    //takes a char and adds the symbol counter to it to create a unique symbol id
    public char genSymID(char c){
        char s[] = new char[6];
        s = s + c;
        s = (to_string(count));
        return s;
    }
    //add symbol to table
    public void addSymbol(sym s){
        symtab.insert(s.symid);
    }
    //fecth symbol
    public sym fetchSymbol(char s[]){
            sym r = symtab.at(s);
            return r;
    }
}


